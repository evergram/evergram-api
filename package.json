{
    "name": "evergram-api",
    "description": "Evergram's api",
    "keywords": [
        "evergram",
        "api"
    ],
    "version": "0.0.1",
    "private": true,
    "author": "Josh Stuart <joshstuartx@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/evergram/evergram-api.git"
    },
    "engines": {
        "node": "0.12.00"
    },
    "scripts": {
        "lint": "./node_modules/.bin/jshint ./app",
        "checkStyle": "./node_modules/.bin/jscs ./app",
        "pretest": "npm run-script lint && npm run-script checkStyle",
        "test": "NODE_ENV=test ./node_modules/.bin/istanbul cover --dir shippable/coverage ./node_modules/.bin/_mocha -- --recursive --reporter spec-xunit-file -r should",
        "start": "NODE_ENV=development ./node_modules/.bin/nodemon server.js"
    },
    "dependencies": {
        "async": "0.9.0",
        "body-parser": "1.8.2",
        "compression": "1.1.0",
        "connect-mongo": "0.8.2",
        "cookie-parser": "1.3.3",
        "cookie-session": "1.0.2",
        "cors": "^2.6.0",
        "evergram-common": "git+ssh://git@github.com:crimsonronin/evergram-common.git#master",
        "express": "4.13.1",
        "express-session": "1.8.2",
        "express-validator": "2.9.0",
        "lodash": "^3.5.0",
        "method-override": "2.2.0",
        "moment": "^2.9.0",
        "newrelic": "^1.18.3",
        "passport": "0.2.1",
        "passport-instagram": "0.1.2",
        "q": "^1.4.1"
    },
    "devDependencies": {
        "istanbul": "^0.3.13",
        "jscs": "^1.12.0",
        "jshint": "^2.7.0",
        "mocha": "^2.2.4",
        "nodemon": "1.2.1",
        "should": "^6.0.1",
        "sinon": "^1.14.1",
        "spec-xunit-file": "0.0.1-3",
        "supertest": "^0.15.0"
    }
}
